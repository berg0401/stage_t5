<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="move_pick">
    <Sequence>
      <move_robot TCP_Position="23;2;-5;180;30;270"
                  service_name="move_robot"/>
      <GrabPiece object_measure="7"/>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="GrabPiece"
            editable="true">
      <input_port name="object_measure"/>
    </Action>
    <Action ID="move_robot"
            editable="true">
      <input_port name="TCP_Position"/>
      <input_port name="service_name"
                  default="move_robot"/>
    </Action>
  </TreeNodesModel>

</root>
